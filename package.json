{
  "name": "review-arcade",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:coverage": "turbo test:coverage",
    "dev:teacher": "turbo dev --filter=@review-arcade/teacher",
    "dev:student": "turbo dev --filter=@review-arcade/student",
    "build:teacher": "turbo build --filter=@review-arcade/teacher",
    "build:student": "turbo build --filter=@review-arcade/student"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "turbo": "^2.3.0"
  },
  "packageManager": "pnpm@9.15.0"
}
